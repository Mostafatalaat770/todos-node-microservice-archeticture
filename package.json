{
    "name": "todo",
    "version": "1.0.0",
    "description": "A simple todo app backend",
    "main": "main.ts",
    "scripts": {
        "build": "npx tsc",
        "pinoPrettify": "npx pino-pretty -i time,hostname,module,__in,name,pid",
        "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/src/main.js | yarn pinoPrettify",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "test": "jest",
        "db:wipe": "docker-compose down && docker volume rm todo_database-data && docker-compose up -d",
        "db:migrate": "npx knex --knexfile ./src/database/knexfile.ts migrate:latest",
        "db:seed": "npx knex --knexfile ./src/database/knexfile.ts seed:run",
        "db:reset": "npm run db:wipe && npm run db:migrate && npm run db:seed",
        "start": "npm run build && npx concurrently \"npm:start:*\"",
        "start:dev": "npm run dev",
        "start:ts": "tsc --watch --incremental",
        "start:mocks": "npx mockserver -p 3001 -m mocks"
    },
    "author": "Mostafa Talaat",
    "license": "ISC",
    "dependencies": {
        "agentkeepalive": "^4.2.1",
        "axios": "^1.2.0",
        "express": "^4.18.2",
        "express-openapi-validator": "^4.13.8",
        "express-pino-logger": "^7.0.0",
        "jsonwebtoken": "^8.5.1",
        "jwks-rsa": "^3.0.0",
        "knex": "^2.3.0",
        "memoizee": "^0.4.15",
        "pg": "^8.8.0",
        "pino": "^8.7.0",
        "swagger-ui-express": "^4.5.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.14",
        "@types/express-pino-logger": "^4.0.3",
        "@types/jest": "^29.2.2",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/lodash": "^4.14.189",
        "@types/memoizee": "^0.4.8",
        "@types/node": "^18.11.9",
        "@types/pg": "^8.6.5",
        "@types/supertest": "^2.0.12",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.13.0",
        "@typescript-eslint/parser": "^5.0.0",
        "concurrently": "^7.5.0",
        "eslint": "^8.26.0",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "2.25.2",
        "eslint-plugin-jest": "^27.1.4",
        "jest": "^29.2.2",
        "lodash": "^4.17.21",
        "nodemon": "^2.0.20",
        "pino-pretty": "^9.1.1",
        "prettier": "^2.7.1",
        "supertest": "^6.3.1",
        "testcontainers": "^9.0.0",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    }
}
